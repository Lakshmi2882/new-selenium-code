pipeline { 
agent any 
    stages { 
        
        stage ('Build') { 
            steps{
                echo "Building"

            }
        }
        
        
            stage ('Deploy to QA') { 
            steps{
                echo "Test"

            }
        }
            
     
            stage ('Test on QA') { 
            steps{
                echo "QA"

            }
        }
        
        
        
        
            stage ('Deploy to stage') { 
            steps{
                echo "Stage"

            }
        }
        
            stage ('Test on stage') { 
            steps{
                echo "Test"

            }
        }
        
            stage ('Monitor') { 
            steps{
                echo "Monitor"

            }
        }
    }

 }